{
  "name": "NetWorkMonitor",
  "description": "Intercept and record network traffic",
  "version": "0.1",
  "permissions": [
    "debugger"
  ],
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  },
   "permissions": [
      "webRequest",
      "*://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "NetWorkMonitor",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy":"script-src 'self' https://www.gstatic.com; object-src 'self'"

}

